{
  "name": "vscode-code-renderer",
  "displayName": "vscode-code-renderer",
  "description": "Source code renderer for VS Code notebooks",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "keywords": [
    "notebookRenderer"
  ],
  "categories": [
    "Other"
  ],
  "publisher": "rebornix",
  "activationEvents": [],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "notebookRenderer": [
      {
        "id": "vscode-code-renderer",
        "entrypoint": "./out/client/index.js",
        "displayName": "vscode-code-renderer",
        "mimeTypes": [
            "text/x-css",
            "text/x-html",
            "text/x-json",
            "text/x-typescript",
            "text/x-abap",
            "text/x-apex",
            "text/x-azcli",
            "text/x-bat",
            "text/x-cameligo",
            "text/x-clojure",
            "text/x-coffee",
            "text/x-cpp",
            "text/x-csharp",
            "text/x-csp",
            "text/x-css",
            "text/x-dart",
            "text/x-dockerfile",
            "text/x-ecl",
            "text/x-fsharp",
            "text/x-go",
            "text/x-graphql",
            "text/x-handlebars",
            "text/x-hcl",
            "text/x-html",
            "text/x-ini",
            "text/x-java",
            "text/x-javascript",
            "text/x-julia",
            "text/x-kotlin",
            "text/x-less",
            "text/x-lexon",
            "text/x-lua",
            "text/x-m3",
            "text/x-markdown",
            "text/x-mips",
            "text/x-msdax",
            "text/x-mysql",
            "text/x-objective-c/objective",
            "text/x-pascal",
            "text/x-pascaligo",
            "text/x-perl",
            "text/x-pgsql",
            "text/x-php",
            "text/x-postiats",
            "text/x-powerquery",
            "text/x-powershell",
            "text/x-pug",
            "text/x-python",
            "text/x-r",
            "text/x-razor",
            "text/x-redis",
            "text/x-redshift",
            "text/x-restructuredtext",
            "text/x-ruby",
            "text/x-rust",
            "text/x-sb",
            "text/x-scala",
            "text/x-scheme",
            "text/x-scss",
            "text/x-shell",
            "text/x-solidity",
            "text/x-sophia",
            "text/x-sql",
            "text/x-st",
            "text/x-swift",
            "text/x-systemverilog",
            "text/x-tcl",
            "text/x-twig",
            "text/x-typescript",
            "text/x-vb",
            "text/x-xml",
            "text/x-yaml"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "monaco-editor-webpack-plugin": "^7.0.1",
    "monaco-editor": "^0.32.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@types/vscode-notebook-renderer": "^1.57.8",
    "@types/vscode": "^1.64.0",
    "css-loader": "^4.2.0",
    "eslint": "^8.1.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "vscode-notebook-error-overlay": "^1.0.1",
    "@vscode/test-electron": "^1.6.2",
    "util": "^0.12.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "terser-webpack-plugin": "^5.3.1",
    "file-loader": "^6.2.0"
  }
}
